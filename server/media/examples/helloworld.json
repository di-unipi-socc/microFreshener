{
    "name": "hello-world",
    "nodes": [
        {
            "name": "shipping",
            "type": "service"
        },
        {
            "name": "order_db",
            "type": "database"
        },
        {
            "name": "order",
            "type": "service"
        },
        {
            "name": "rabbitmq",
            "type": "messagebroker"
        },
        {
            "name": "gateway",
            "type": "messagerouter"
        }
    ],
    "links": [
        {
            "source": "shipping",
            "target": "order_db",
            "type": "runtime"
        },
        {
            "source": "shipping",
            "target": "rabbitmq",
            "type": "runtime"
        },
        {
            "source": "order",
            "target": "shipping",
            "type": "runtime",
            "timeout": true
        },
        {
            "source": "order",
            "target": "order_db",
            "type": "runtime"
        },
        {
            "source": "order",
            "target": "rabbitmq",
            "type": "runtime"
        },
        {
            "source": "gateway",
            "target": "shipping",
            "type": "runtime"
        }
    ],
    "groups": [
        {
            "name": "edgenodes",
            "type": "edgegroup",
            "members": [
                "shipping",
                "order",
                "gateway"
            ]
        },
        {
            "name": "team1",
            "type": "squadgroup",
            "members": [
                "shipping",
                "rabbitmq"
            ]
        },
        {
            "name": "team2",
            "type": "squadgroup",
            "members": [
                "order",
                "order_db"
            ]
        }
    ]
}